{
    "name": "lupennat/nested-form",
    "description": "Laravel Nova - Nested Form",
    "keywords": [
        "laravel",
        "nova"
    ],
    "authors": [
        {
          "name": "Claudio Pennati",
          "email": "claudio.pennati@gmail.com"
        }
      ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "laravel/nova": "^4.12",
        "nova-kit/nova-packages-tool": "^1.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.16"
    },
    "autoload": {
        "psr-4": {
            "Lupennat\\NestedForm\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lupennat\\NestedForm\\NestedFormServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
    {
        "type": "composer",
        "url": "https://nova.laravel.com"
    }
    ],
    "scripts": {
        "php-cs-fixer": "vendor/bin/php-cs-fixer fix"
    }
}
